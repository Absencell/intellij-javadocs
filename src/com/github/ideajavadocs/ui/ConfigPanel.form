<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.github.ideajavadocs.ui.ConfigPanel">
  <grid id="27dc6" binding="panel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <tabbedpane id="c7b04" binding="tabbedPane">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="6e0b3" binding="generalPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints>
              <tabbedpane title="General"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="3927e" class="javax.swing.JCheckBox" binding="alwaysReplaceJavadocsCheckBox" default-binding="true">
                <constraints border-constraint="Center"/>
                <properties>
                  <text value="Always replace javadocs"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="e41e2" binding="templatesPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints>
              <tabbedpane title="Templates"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children/>
          </grid>
          <grid id="a7bb4" binding="replacementsPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints>
              <tabbedpane title="Replacements"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children/>
          </grid>
        </children>
      </tabbedpane>
    </children>
  </grid>
</form>
